{
    "name": "andrey-helldar/laravel-helpers",
    "description": "Deprecated Laravel 6 helpers",
    "keywords": ["laravel", "helpers"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/andrey-helldar/laravel-helpers/issues",
        "source": "https://github.com/andrey-helldar/laravel-helpers"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/support": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\LaravelHelpers\\": "src/"
        },
        "files": [
            "src/Support/helpers_arr.php",
            "src/Support/helpers_string.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\LaravelHelpers\\ServiceProvider"
            ]
        }
    }
}
